{
	"name": "portfolio-2.0",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
		"prisma:generate": "dotenv -e .env.local -- npx prisma generate",
		"prisma:db:seed": "dotenv -e .env.local -- npx prisma db seed",
		"prisma:db:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
		"vercel-build": "npm run prisma:db:deploy && next build"
	},
	"prisma": {
		"schema": "src/prisma/schema.prisma",
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "^5.2.0",
		"@types/node": "20.5.9",
		"@types/react": "18.2.21",
		"@types/react-dom": "18.2.7",
		"classnames": "^2.3.2",
		"eslint": "8.48.0",
		"eslint-config-next": "13.4.19",
		"next": "13.4.19",
		"nodemailer": "^6.9.5",
		"normalize.css": "^8.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.46.1",
		"react-tabs": "^6.0.2",
		"swiper": "^10.3.0",
		"typescript": "5.2.2"
	},
	"devDependencies": {
		"@types/nodemailer": "^6.4.10",
		"dotenv-cli": "^7.3.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"prettier": "^3.0.3",
		"prisma": "^5.2.0",
		"sass": "^1.66.1",
		"ts-node": "^10.9.1"
	}
}
